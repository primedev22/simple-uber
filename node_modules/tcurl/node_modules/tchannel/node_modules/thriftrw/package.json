  "dependencies": {
    "bufrw": "^0.9.4",
    "error": "7.0.2",
    "long": "^2.4.0",
    "pegjs": "^0.8.0"
  },
  "devDependencies": {
    "coveralls": "^2.10.0",
    "debug": "^2.1.2",
    "fast-stats": "0.0.3",
    "faucet": "0.0.1",
    "istanbul": "^0.3.5",
    "itape": "^1.5.0",
    "jsen": "^0.6.0",
